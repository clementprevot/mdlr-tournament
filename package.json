        "registry": "https://registry.npmjs.org"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/clementprevot/mdlr-tournaments.git"
    },
    "scripts": {
        "build": "pnpm run prebuild && NODE_ENV=production pnpm next build",
        "build-vercel": "pnpm next build",
        "check-deps": "pnpm knip --dependencies",
        "clean-build": "rm -Rf ./dist >/dev/null 2>&1 || true",
        "clean-next": "rm -Rf ./.next >/dev/null 2>&1 || true",
        "dev": "pnpm next dev --turbopack",
        "format": "pnpm prettier --cache --check .",
        "format-fix": "pnpm prettier --cache --write .",
        "init-supabase": "echo 'n n' | pnpm supabase init --force && pnpm run link-supabase",
        "knip-report": "pnpm knip --reporter markdown > knip.md",
        "link-supabase": "./scripts/link-supabase.sh",
        "link-vercel": "pnpm vercel link",
        "lint": "pnpm oxlint",
        "lint-fix": "pnpm oxlint --fix",
        "login-supabase": "pnpm supabase login",
        "login-vercel": "pnpm vercel whoami",
        "login-vercel-force": "pnpm vercel login",
        "prebuild": "(pnpm run clean-build && mkdir -p ./dist) & pnpm run update-env",
        "predev": "pnpm run clean-next",
        "prepare": "husky",
        "presetup": "pnpm install",
        "pretypes-check": "pnpm is-ci && pnpm run build || true",
        "quality": "pnpm run lint && pnpm run types-check",
        "quality-fix": "pnpm run lint-fix && pnpm run types-check",
        "setup": "pnpm is-ci && pnpm run setup-ci || pnpm run setup-non-ci",
        "setup-ci": "true",
        "setup-husky": "pnpm exec husky init",
        "setup-non-ci": "pnpm run setup-husky && pnpm run setup-vercel && pnpm run setup-supabase",
        "setup-supabase": "pnpm run login-supabase && pnpm run link-supabase && pnpm run update-typings-supabase",
        "setup-vercel": "pnpm run login-vercel && pnpm run link-vercel && pnpm run update-env",
        "start": "pnpm next start",
        "types-check": "pnpm run pretypes-check && pnpm tsc --noEmit",
        "update-deps": "pnpm update --interactive",
        "update-env": "pnpm vercel env pull",
        "update-typings": "pnpm run update-typings-supabase",
        "update-typings-supabase": "./scripts/update-typings-supabase.sh"
    },
    "type": "module",
    "version": "0.1.0"
}
